{
    "name": "mayoz/mocking-guzzle",
    "description": "Testing the Guzzle requests with PHPUnit.",
    "keywords": [
        "mocking",
        "guzzle",
        "phpunit",
        "response"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Sercan Cakir",
            "email": "srcnckr@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
